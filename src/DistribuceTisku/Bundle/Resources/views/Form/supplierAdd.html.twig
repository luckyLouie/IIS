
{% extends 'DistribuceTiskuBundle::layout.html.twig' %}

{% block title %}Přidat dodavatele{% endblock%}


{% block body %}
    <header>
        <h1>Přidat doručovatele</h1>
    </header>

    {% for flashMessage in app.session.flashbag.get('ok') %}
    <div class="blogger-notice">
    {{ flashMessage }}
    </div>
    {% endfor %}

    {% for flashMessage in app.session.flashbag.get('exception') %}
    <div class="exception-notice">
    {{ flashMessage }}
    </div>
    {% endfor %} 
{% block form %}
    <form action="{{ path('_supplierAdd') }}" method="post" {{ form_enctype(form) }} class="blogger">
{% endblock %} 
        {{ form_errors(form) }}

        {{ form_row(form.jmeno) }}
        {{ form_row(form.prijmeni) }}
        {{ form_row(form.adresa) }}
        {{ form_row(form.psc) }}
        {{ form_row(form.telefon, {'required': false}) }}
{% block rest_fort %}
        {{ form_rest(form) }}
{% endblock %}
        <input type="submit" value="Submit" />
    </form>
{% endblock %}{# empty Twig template #}
